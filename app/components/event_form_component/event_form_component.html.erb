<%= simple_form_for @event, url: @path, method: @method do |f| %>
	<%= f.input :title %>
	<%= f.input :description %>
	<%= f.input :location %>
	<%= f.input :start_at, html5: true %>
	<%= f.input :end_at, html5: true %>
	<%= f.input :creator_name %>
	<%= f.input :creator_email %>
	<%= f.button :submit %>
<% end %>
